<?xml version="1.0" encoding="utf-8"?>
<androidx.preference.PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <PreferenceCategory
        android:title="@string/pref_category_basic_info"
        android:key="pref_basic_info" >
    </PreferenceCategory>


    <PreferenceCategory
        android:title="@string/pref_category_location"
        android:key="pref_location">

        <com.silverback.carman2.views.DistrictDialogPreference
            android:title="@string/pref_dialog_district"
            android:key="pref_dialog_district" />


    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/pref_category_station"
        android:key="pref_station" >
    </PreferenceCategory>


    <PreferenceCategory
        android:title="@string/pref_category_service"
        android:key="pref_service" >

    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/pref_category_app_setting"
        android:key="pref_app_setting" >

        <SwitchPreferenceCompat
            app:key="notification"
            app:title="Enable message notification" />

    </PreferenceCategory>

</androidx.preference.PreferenceScreen>